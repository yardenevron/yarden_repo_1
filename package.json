		"@rollup/plugin-json": "^6.0.0",
		"@rollup/plugin-node-resolve": "^15.0.1",
		"@svitejs/changesets-changelog-github-compact": "^1.1.0",
		"playwright": "^1.29.2",
		"prettier": "^2.8.0",
